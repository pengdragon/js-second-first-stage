<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title></title>
	</head>
	<body>
		<script>
			var arrNum=[1,1,1,3,3,3,4,5,5,5,1,1,1,1];//百度面试题
			function countArr(){
				var obj = {};
				for(var i=0;i<arrNum.length;i++){
					if(arrNum[i] in obj){
						obj[arrNum[i]]=obj[arrNum[i]]+1;
					}
					else{
						obj[arrNum[i]]=1;
					}
				}return obj;
			}
			var obj=countArr();
			console.log(obj);
		</script>
	</body>
</html>
